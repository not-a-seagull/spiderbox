{
  "compilerOptions": {
    "allowJs": true,
    "target": "es3",
    "removeComments": true,
    "sourceMap": false,
    "strict": true,
    "strictNullChecks": false,
    "baseUrl": ".",
    "outDir": "./",
    "noEmitOnError": true,
    "module": "umd",
    "downlevelIteration": true,
    "noImplicitAny": true,
    "declaration": true,

    "lib": [ "dom", "es2015" ]
  },
  "include": [
    "src/index.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
